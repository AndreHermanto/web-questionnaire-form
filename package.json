{
  "name": "web-questionnaire-form",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/react": "^3.1.5",
    "husky": "^0.13.3",
    "lint-staged": "^3.5.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.3.1",
    "react-scripts": "^1.0.7",
    "react-test-renderer": "15.4.2",
    "redux-mock-store": "^1.2.3"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "create-react-class": "^15.5.3",
    "cuid": "^1.3.8",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "lodash.get": "^4.4.2",
    "normalizr": "^3.2.3",
    "prop-types": "^15.5.10",
    "rc-progress": "^2.1.2",
    "react": "^15.4.2",
    "react-bootstrap": "^0.31.0",
    "react-cookie": "1.0.4",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.5",
    "react-scroll": "^1.5.3",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/scss/index.scss src/scss/../index.css",
    "watch-css": "npm run build-css && node-sass-chokidar src/scss/index.scss src/scss/../index.css --watch --recursive",
    "start-js": "npm run json-server & REACT_APP_BASE_URL=http://localhost:4000 react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "json-server": "node node_modules/web-questionnaire-builder-mockserver",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "homepage": "/questionnaire-form"
}
